<div class="row">
    <div class="col-lg-12 col-md-12">
        <section id="servicios" style="padding-top:0px;padding-bottom:0px;">
            <div class="container-fluid">
                <div class="row" style="padding:0px;padding-bottom:50px;">
                    <div class="col-lg-12 text-center" style="height:50px;">
                        <h2 class="section-heading">Contáctenos </h2>
                        <h3 class="section-subheading text-muted" style="margin-bottom:20px;">Lorem ipsum dolor sit amet consectetur. </h3>
                    </div>
                </div>
                <div class="row text-center">
                    <div class="col-md-6 col-md-offset-3">
                        <form method="post" style="background-color:none;">
                            <div class="form-group has-success has-feedback">
                                <input class="form-control" type="text" name="name" placeholder="Nombre"><i class="form-control-feedback glyphicon glyphicon-ok" aria-hidden="true"></i>
                            </div>
                            <div class="form-group has-error has-feedback">
                                <input class="form-control" type="email" name="email" placeholder="Email"><i class="form-control-feedback glyphicon glyphicon-remove" aria-hidden="true"></i>
                                <p class="help-block">Por favor utilice un email válido..</p>
                            </div>
                            <div class="form-group">
                                <textarea class="form-control" rows="6" name="message" placeholder="Mensaje"></textarea>
                            </div>
                            <div class="form-group">
                                <button class="btn btn-primary" type="submit">Envíar mensaje</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </section>
    </div>
</div><!--Services article-->